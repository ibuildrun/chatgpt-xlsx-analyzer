DirectoryIndex index.php

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    # Skip rewrite for actual PHP files
    RewriteCond %{REQUEST_URI} \.php$ [NC]
    RewriteRule ^ - [L]

    # Route all other requests through index.php (Next.js proxy)
    # This includes /_next/static/* which Next.js serves internally
    RewriteRule ^(.*)$ index.php [L,QSA]
</IfModule>
